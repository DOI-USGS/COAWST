SFC                 = pgfortran
CC                  = mpicc
SCC                 = pgcc
LD                  = $(FC)
FFLAGS              = -Mfree -byteswapio
F77FLAGS            = -Mfixed -byteswapio
FNGFLAGS            = $(FFLAGS)
LDFLAGS             =
