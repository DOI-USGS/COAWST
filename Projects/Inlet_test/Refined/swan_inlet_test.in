PROJECT 'Inlet Test' ' '
 'INLET test'
 'Bathymetry: flat bottom'
 'COMMENTS'

MODE NONSTATIONARY TWODIMENSIONAL

SET DEPMIN 0.10 INRHOG 1 NAUTICAL
COORDINATES CARTESIAN

&& KEYWORD for number of nested SWAN grids.
NSGRIDS 2

&& KEYWORDS TO CREATE AND READ COMPUTATIONAL GRID &&
CGRID CURVILINEAR 76 71 EXC 9.999000e+003 &
        CIRCLE 36 0.04 1.0 20
READGRID COORDINATES 1 'Projects/Inlet_test/Refined/inlet_test_grid_coord.grd' 4 0 0 FREE

&& KEYWORDS TO CREATE AND READ BATHYMETRY GRID &&
INPGRID BOTTOM CURVILINEAR 0 0 76 71 EXC 9.999000e+003
READINP BOTTOM  1 'Projects/Inlet_test/Refined/inlet_test_bathy.bot' 4 0 FREE

&& KEYWORD TO CREATE CURRENT GRID &&
INPGRID CURRENT CURVILINEAR 0 0 76 71 EXC 9.999000e+003                 &
        NONSTAT  20000101.000000 25 DAY 20000126.000000

&& KEYWORD TO CREATE WATER LEVEL GRID &&
INPGRID WLEV CURVILINEAR 0 0 76 71 EXC 9.999000e+003                 &
        NONSTAT  20000101.000000 25 DAY 20000126.000000

&& KEYWORD TO CREATE BOTTOM FRICTION GRID &&
INPGRID FRIC CURVILINEAR 0 0 76 71 EXC 9.999000e+003                 &
        NONSTAT  20000101.000000 25 DAY 20000126.000000
 
&& BOUNDARY FORCING &&
BOUNDPAR1 SHAPESPEC JONSWAP 3.3 PEAK DSPR DEGREES
BOUNDPAR2 SEGMENT IJ 0 71 76 71 CONSTANT PAR 1.0 10.0 0. 20.
 
& Restart name **********************************
&INIT HOTSTART 'swan_restart.dat'

& PHYSICS  **************************************
GEN3
FRICTION MADSEN 0.05
OFF QUAD

BLOCK 'COMPGRID' NOHEADER 'depth01.mat'  LAY 4 DEPTH 1.    OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'watlev01.mat' LAY 4 WATLEV 1.   OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'botlev01.mat' LAY 4 BOTLEV 1.   OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'vel01.mat'    LAY 4 VEL 1.      OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'fric01.mat'   LAY 4 FRCOEF  1.  OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'dissip01.mat' LAY 4 DISSIP 1.   OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'hsig01.mat'   LAY 4 HSIGN 1.    OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'rtp01.mat'    LAY 4 RTP 1.      OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'tmbot01.mat'  LAY 4 TMBOT 1.    OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'ubot01.mat'   LAY 4 UBOT 1.     OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'wdir01.mat'   LAY 4 DIR 1.      OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'wlen01.mat'   LAY 4 WLEN 1.     OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'qb01.mat'     LAY 4 QB 1.       OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'xp01.mat'     LAY 4 XP 1.
BLOCK 'COMPGRID' NOHEADER 'yp01.mat'     LAY 4 YP 1.

POINTS  'point1' 5000.0 10000.0
SPECOUT 'point1' SPEC2D 'point1.spc2d' OUTPUT 20000101.000000 1 HR
TABLE   'point1' HEADER 'point1.table' HSIGN DIR WLEN OUTPUT 20000101.000000 1 HR

PROP BSBT

COMPUTE NONSTATIONARY 20000101.000000 60 SEC 20000101.120000

STOP
