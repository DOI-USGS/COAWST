      SUBROUTINE MNMX(DATA,N,ITTL)
Crizvi      REAL*4 DATA(N)
      REAL DATA(N)
      CHARACTER*(*) ITTL
C
      DMAX=DATA(1)
C      DMIN=DATA(1)
      DMIN=0
      DO 100 I=1,N
       DMAX=MAX(DMAX,DATA(I))
       DMIN=DMIN+DATA(I)
  100 CONTINUE
      WRITE(6,101)ITTL,DMAX,DMIN
  101 FORMAT(1H ,A4,4X,2E12.5)
C
      RETURN
      END SUBROUTINE MNMX

