CC                  = CONFIGURE_CC
LD                  = $(FC)
FFLAGS              = -Mfree -g
F77FLAGS            = -Mfixed -g
FCSUFFIX            = 
FNGFLAGS            = $(FFLAGS)
LDFLAGS             = -g
CFLAGS              = -g
CPP                 = cpp -P -traditional
CPPFLAGS            = -D_UNDERSCORE -DBYTESWAP -DWIN32 -DIO_NETCDF -DIO_BINARY -DIO_GRIB1 -DBIT32 CONFIGURE_MPI
ARFLAGS             =

